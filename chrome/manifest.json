{
    "manifest_version": 2,
    "name": "Easy Rss",
    "description":
        "简易的RSS阅读器",
    "version": "1.0.0",
  
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "index.html"
    },
  
    "background": {
        "scripts": ["background.js"]
    },
  
    "icons": {
        "16": "icon16.png"
       
    },
  
